{
    "name": "diossystem/page",
    "description": "The Page component",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Oleg Dmitrochenko",
            "email": "dmitrochenkooleg@gmail.com"
        }
    ],
    "require": {
       "php": "7 - 8",
       "illuminate/database": "5 - 8",
       "illuminate/support": "5 - 8",
       "illuminate/http": "5 - 8",
       "belca/support-constants": "1.*",
       "diossystem/multicasting": "1.*"
    },
    "require-dev": {
        "orchestra/testbench": "3.5.x-dev",
        "orchestra/database": "~3.5"
    },
    "autoload": {
        "psr-4": {
            "Dios\\System\\Page\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "database/seeds/",
            "database/factories/"
        ],
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dios\\System\\Page\\PageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
